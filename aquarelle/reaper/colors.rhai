const   main = cfg::scheme("main");
const accent = cfg::option("accent");

private fn out() {
	let rgb = rgb::coords(this);
	rgb.red | rgb.green << 8 | rgb.blue << 16
}

private fn shadow(factor) {
	const L = this.luma();
	lab::mix(this, 0, (2 ** -L + 0.2 * L - 0.6) * factor).out()
}

